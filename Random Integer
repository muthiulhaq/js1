<body>
<input type="number" id="myNumber"/>
<input type="button" value="Check" onclick="checkme()"/>

<script>
function checkme()
{
var myval = document.getElementById("myNumber").value;
var randnum = Math.floor((Math.random() * 10) + 1);
alert(randnum);
	if(myval==randnum)
	{
		alert("Same")
	}
	else
	{
		alert("Not same");
	}
}
</script>

